CurrentDate = DateTimeConvert(2,DateNum());
Difference = round(DateTimeDiff(NextDay,CurrentDate)/(3600*24));
TimeFrameRestore();

Difference = TimeFrameExpand(Difference,inDaily,expandFirst);

